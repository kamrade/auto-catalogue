{
  "semi": true,
  "singleQuote": false,
  "bracketSpacing": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "none",
  "printWidth": 120,
  "arrowParens": "always",
  "plugins": ["prettier-plugin-svelte"],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
  "svelteBracketNewLine": true,
  "singleAttributePerLine": false
}
